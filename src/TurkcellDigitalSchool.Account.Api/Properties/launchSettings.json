{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "IIS Express": {
    "commandName": "IISExpress",
    "launchBrowser": true,
    "launchUrl": "swagger",
    "environmentVariables": {
      "ASPNETCORE_ENVIRONMENT": "Development"
    }
  },
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iis": {
      "applicationUrl": "http://localhost/TurkcellDigitalSchool.Auth.Api",
      "sslPort": 0
    },
    "iisExpress": {
      "applicationUrl": "http://localhost:6021",
      "sslPort": 0
    }
  },
  "profiles": { 
    "TurkcellDigitalSchool.Api.DEV": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "DEV"
      },
      "applicationUrl": "http://localhost:6021",
      "dotnetRunMessages": true
    },
    "TurkcellDigitalSchool.Api.DEVTURKCELL": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "DEVTURKCELL"
      },
      "applicationUrl": "http://localhost:6021",
      "dotnetRunMessages": true
    }
  }
}